{
  "name": "multi-agent-ide",
  "version": "1.0.0",
  "description": "A collaborative IDE with multiple AI agents",
  "main": "dist/main.js",
  "homepage": "./",
  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config webpack.config.js --mode production",
    "build:renderer": "webpack --config webpack.config.js --mode production",
    "build:electron": "npm run build && electron-builder",
    "dev": "node scripts/start-with-encoding.js",
    "dev:original": "node scripts/dev.js",
    "dev:win": "scripts\\dev-win.bat",
    "dev:ps": "powershell -ExecutionPolicy Bypass -File scripts/dev.ps1",
    "dev:full": "node scripts/start-dev.js",
    "dev:electron": "node scripts/dev.js",
    "dev:main": "webpack --config webpack.config.js --mode development --watch",
    "dev:renderer": "webpack serve --config webpack.config.js --mode development",
    "test": "jest",
    "test:integration": "node scripts/test-integration.js",
    "test:e2e": "ts-node src/tests/e2e/run-e2e-tests.ts",
    "test:e2e:collaboration": "ts-node src/tests/e2e/run-e2e-tests.ts --suite collaboration",
    "test:e2e:concurrent": "ts-node src/tests/e2e/run-e2e-tests.ts --suite concurrent",
    "test:e2e:performance": "ts-node src/tests/e2e/run-e2e-tests.ts --suite performance",
    "test:e2e:verbose": "ts-node src/tests/e2e/run-e2e-tests.ts --verbose",
    "test:e2e:update-baseline": "ts-node src/tests/e2e/run-e2e-tests.ts --suite performance --update-baseline",
    "validate:e2e": "node scripts/validate-e2e-system.js",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "start": "electron dist/main.js",
    "start:electron": "electron .",
    "prepare": "husky install"
  },
  "keywords": [
    "ide",
    "ai",
    "agents",
    "collaboration",
    "electron",
    "desktop"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dnd": "^2.0.36",
    "@types/react-dnd-html5-backend": "^3.0.2",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "concurrently": "^8.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^7.0.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.8.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.0.0",
    "electron-updater": "^6.1.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-html-reporters": "^3.1.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.0.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "wait-on": "^7.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-window": "^1.8.8",
    "antd": "^5.12.0",
    "bull": "^4.12.0",
    "chokidar": "^3.5.0",
    "dayjs": "^1.11.18",
    "diff": "^5.1.0",
    "dotenv": "^16.3.0",
    "eventemitter3": "^5.0.0",
    "express": "^4.18.0",
    "fs-extra": "^11.0.0",
    "fuse.js": "^7.0.0",
    "html2canvas": "^1.4.1",
    "immer": "^10.0.0",
    "monaco-editor": "^0.45.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.0",
    "react-window": "^2.1.1",
    "recharts": "^2.15.4",
    "redis": "^4.6.0",
    "rxjs": "^7.8.0",
    "simple-git": "^3.20.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "sqlite3": "^5.1.0",
    "styled-components": "^6.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "build": {
    "appId": "com.multiagent.ide",
    "productName": "Multi-Agent IDE",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
